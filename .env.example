# Content Moderation Benchmark - Environment Variables
# Copy this file to .env and fill in your actual credentials
# IMPORTANT: Never commit .env to version control!

# =============================================================================
# Shumei (数美科技)
# =============================================================================
SHUMEI_ACCESS_KEY=your_shumei_access_key_here
SHUMEI_APP_ID=default
SHUMEI_TEXT_URL=https://api-text-bj.fengkongcloud.com/text/v4
SHUMEI_IMAGE_URL=https://api-img-bj.fengkongcloud.com/image/v4

# =============================================================================
# Bytedance (字节跳动)
# =============================================================================
# BYTEDANCE_ACCESS_KEY=your_bytedance_access_key_here
# BYTEDANCE_SECRET_KEY=your_bytedance_secret_key_here
# BYTEDANCE_TEXT_URL=https://xxx.bytedance.com/text
# BYTEDANCE_IMAGE_URL=https://xxx.bytedance.com/image

# =============================================================================
# NetEase Yidun (网易易盾)
# =============================================================================
# YIDUN_SECRET_ID=your_yidun_secret_id_here
# YIDUN_SECRET_KEY=your_yidun_secret_key_here
# YIDUN_BUSINESS_ID_TEXT=your_text_business_id_here
# YIDUN_BUSINESS_ID_IMAGE=your_image_business_id_here

# =============================================================================
# JunTong (君同未来)
# =============================================================================
# JUNTONG_TEXT_API_KEY=your_juntong_text_api_key_here
# JUNTONG_IMAGE_API_KEY=your_juntong_image_api_key_here
# JUNTONG_BASE_URL=http://121.40.172.175:8269

# =============================================================================
# Huoshan/Volcengine (火山引擎 LLM Shield)
# =============================================================================
# HUOSHAN_ACCESS_KEY=your_volcengine_access_key_here
# HUOSHAN_SECRET_KEY=your_volcengine_secret_key_here
# HUOSHAN_APP_ID=your_llmshield_app_id_here
# HUOSHAN_REGION=cn-beijing

# =============================================================================
# Benchmark Configuration
# =============================================================================
# Concurrency settings
MAX_WORKERS=10
REQUEST_INTERVAL=0.1
REQUEST_TIMEOUT=30
RETRY_TIMES=3

# Output settings
OUTPUT_DIR=output
REPORT_DIR=reports
